<%- include('navbar-sidenav') -%>
    <h2>Welocme Admin,</h2>
    <h3>Books List</h3>
    <table>
        <tr>
            <th>Book ID</th>
            <th>Title</th>
            <th>Stock</th>
            <th>Publication Date</th>
        </tr>
        <% books.forEach(function(book) { %>
            <tr>
                <td><%= book.bookId %></td>
                <td><%= book.bookTitle %></td>
                <td><%= book.bookStock %></td>
                <td><%= book.bookPublicationDate %></td>
            </tr>
        <% }); %>
    </table>

<%- include('footer') -%>